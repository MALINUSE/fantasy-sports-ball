<h1>Login</h1>

<%= render "./shared/errors" %>

<div class="login">
  <%= form_tag login_auth_path do %>

        <%= label_tag :username %>
        <%= text_field_tag :username %>
        <br>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
        <br>
    <%= submit_tag "Log in" %>
  <% end %>
</div>
